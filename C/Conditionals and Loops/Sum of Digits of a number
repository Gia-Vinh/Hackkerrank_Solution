#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    int re, digits_sum;
    digits_sum = 0;
    while (n > 0){
        re = n%10;
        n = n/10;
        digits_sum += re;
    }
    printf("%d",digits_sum);
    
    
    return 0;
}
