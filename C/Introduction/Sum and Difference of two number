#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a, b;
    scanf("%d %d",&a, &b);
    printf("%d %d",a+b,a-b);
    
    scanf("\n");
    printf("\n");
    
    float c, d;
    scanf("%f %f", &c, &d);
    printf("%.1f %.1f",c+d, c-d);
	
    
    return 0;
}
