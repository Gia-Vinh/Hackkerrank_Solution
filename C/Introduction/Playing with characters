#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char ch;
    scanf("%c",&ch);
    printf("%c",ch);
    scanf("\n");
    printf("\n");
    
    char s[100];
    scanf("%[^\n]%*c", s);
    printf("%s",s);
    scanf("\n");
    printf("\n");
    
    char sen[100];
    scanf("%[^\n]%*c", sen);
    printf("%s",sen);
    
    return 0;
}
